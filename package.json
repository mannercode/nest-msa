        "@types/multer": "^1.4.12",
        "@types/node": "^22.10.1",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.18.0",
