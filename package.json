{
    "name": "nest-seed",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node _output/dist/index.js",
        "build": "rm -rf _output/dist && nest build --webpack",
        "lint": "prettier --write 'src/**/*.ts' && eslint",
        "infra": "bash scripts/run_infra.sh && bash scripts/reset_output.sh",
        "watch:app": "npm run infra && nest start --entryFile app/development --debug 9230 --watch",
        "watch:test": "npm run infra && node --inspect=9330 -r ts-node/register ./node_modules/.bin/jest --watch --onlyChanged",
        "test:all": "npm run infra && jest --coverage --config ./jest.config.ts",
        "test:app": "npm run infra && jest --coverage --config ./jest.config.app.ts",
        "test:common": "npm run infra && jest --coverage --config ./jest.config.common.ts",
        "test:100x": "bash -c 'for i in {1..100}; do npm run test:all || { echo \"Test failed at iteration $i\"; exit 1; }; done'"
    },
    "TODO": "chalk,nanoid 등 esm은 node에서 사용 가능하나 jest는 아직이다. node 24에서 jest 다시 점검해라",
    "dependencies": {
        "@nestjs-modules/ioredis": "^2.0.2",
        "@nestjs/bullmq": "^10.2.3",
        "@nestjs/common": "^10.4.15",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.4.15",
        "@nestjs/event-emitter": "^2.1.1",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mapped-types": "^2.0.6",
        "@nestjs/microservices": "^10.4.15",
        "@nestjs/mongoose": "^10.1.0",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.4.15",
        "@nestjs/terminus": "^10.2.3",
        "bcrypt": "^5.1.1",
        "bullmq": "^5.33.0",
        "chalk": "4.1.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.7.5",
        "express": "^4.21.2",
        "ioredis": "^5.4.1",
        "joi": "^17.13.3",
        "lodash": "^4.17.21",
        "mongoose": "^8.8.4",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.13.1",
