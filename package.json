{
    "name": "nest-seed",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "node _output/dist/app/main",
        "build": "nest build --webpack",
        "lint": "eslint projects/**/*/*.ts --fix",
        "format": "prettier --write projects/**/*/*.ts",
        "watch:app": "nest start --debug 9230 --watch",
        "watch:test": "bash scripts/reset_output.sh && node --inspect=9330 -r ts-node/register ./node_modules/.bin/jest --watch --onlyChanged",
        "test:all": "bash scripts/reset_output.sh && jest --coverage --config ./jest.config.ts",
        "test:app": "bash scripts/reset_output.sh && jest --coverage --config ./jest.config.app.ts",
        "test:common": "bash scripts/reset_output.sh && jest --coverage --config ./jest.config.common.ts"
    },
    "dependencies": {
        "@nestjs-modules/ioredis": "^2.0.2",
        "@nestjs/bullmq": "^10.2.2",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.2.3",
        "@nestjs/core": "^10.3.10",
        "@nestjs/event-emitter": "^2.0.4",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mapped-types": "^2.0.5",
        "@nestjs/microservices": "^10.4.1",
        "@nestjs/mongoose": "^10.0.10",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.3.10",
        "@nestjs/terminus": "^10.2.3",
        "@nestjs/typeorm": "^10.0.2",
        "bcrypt": "^5.1.1",
        "bullmq": "^5.23.0",
        "chalk": "4.1.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "compression": "^1.7.4",
        "express": "^4.19.2",
        "ioredis": "^5.4.1",
        "joi": "^17.13.3",
        "lodash": "^4.17.21",
        "mongoose": "^8.4.5",
        "mongoose-delete": "^1.0.2",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.12.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.20",
        "winston": "^3.13.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.4.2",
        "@nestjs/schematics": "^10.1.2",
        "@nestjs/testing": "^10.3.10",
        "@testcontainers/redis": "^10.13.2",
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.17.6",
        "@types/mongoose-delete": "^1.0.6",
        "@types/multer": "^1.4.11",
        "@types/node": "^22.6.1",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.7.0",
        "@typescript-eslint/parser": "^8.7.0",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "jest": "^29.7.0",
        "mongodb-memory-server": "^10.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.3.2",
        "source-map-support": "^0.5.21",
